                    if (button) {
                      $.ajax({
                        type: "POST",
                        url: "/register-competition",
                        data: {
                          competition: "school-champ",
                        },
                        success: function (result) {
                          if (result) {
                            new swal({
                              title: "Success",
                              text: "Registered successfully",
                              icon: "success",
                            }).then((flag) => {
                              if (flag) {
